{
  "expo": {
    "name": "test-tempo",
    "slug": "vizinhoalert-rzi1z1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.autisyncsorganization.vizinhoalertrzi1z1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "VizinhoAlert needs camera access to scan vehicle QR codes for sending alerts.",
